<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Registration List</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <div class="container">
    <h1>Event Registrations</h1>
    <a href="/auth/logout" class="btn logout-btn">Logout</a>
    <a href="/registrations/search" class="btn">Search Registrations</a>
    <table>
      <thead>
        <tr>
          <th>Student</th>
          <th>Event</th>
          <th>Registration Date</th>
        </tr>
      </thead>
      <tbody>
        <% registrations.forEach(reg => { %>
          <tr>
            <td><%= reg.studentId.username %></td>
            <td><%= reg.eventId.name %></td>
            <td><%= new Date(reg.registrationDate).toLocaleString() %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</body>
</html>